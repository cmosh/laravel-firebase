{
    "name": "cmosh/laravel-firebase",
    "description": "A Firebase port for Laravel (4.2+)",
    "authors": [{
        "name": "Clive Makamara",
        "email": "clive@makamara.me"
    }],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/firebase/php-jwt"
    }],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "6.1.*",
        "firebase/php-jwt": "@dev"
    },
    "autoload": {
        "classmap": [
            "src/cmosh"
        ],
        "psr-0": {
            "Cmosh\\LaravelFirebase": "src/cmosh/LaravelFirebase"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}